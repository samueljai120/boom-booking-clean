# Production Environment Variables for Boom Booking Clean
# Copy this to .env.local for local development

# Database Configuration
DATABASE_URL=your_neon_postgresql_connection_string_here

# JWT Configuration
JWT_SECRET=your_super_secure_jwt_secret_key_here_change_this_in_production
JWT_EXPIRES_IN=7d

# Stripe Configuration (for subscription billing)
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key_here
STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here
STRIPE_WEBHOOK_SECRET=whsec_your_stripe_webhook_secret_here

# Application Configuration
VITE_API_BASE_URL=/api
VITE_WS_URL=
VITE_APP_NAME=Boom Karaoke Booking
VITE_APP_VERSION=1.0.0

# Environment
NODE_ENV=production
PORT=3000

# Security
CORS_ORIGIN=*
ALLOWED_ORIGINS=*

# Monitoring (optional)
SENTRY_DSN=your_sentry_dsn_here
ANALYTICS_ID=your_analytics_id_here
